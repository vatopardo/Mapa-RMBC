            attribution: '',
            interactive: true,
            dataVar: 'json_Soachacasos_criticos_cundinamarca__municipios_de_cundinamarca_2022_copiar_0',
            layerName: 'layer_Soachacasos_criticos_cundinamarca__municipios_de_cundinamarca_2022_copiar_0',
            pane: 'pane_Soachacasos_criticos_cundinamarca__municipios_de_cundinamarca_2022_copiar_0',
