            resetLabels([layer_Soachacasos_criticos_cundinamarca__municipios_de_cundinamarca_2022_copiar_0,layer_Elguaviocasos_criticos_cundinamarca__energaparalacapitalabandonoparalaregin_1,layer_Impactoambientalsincompensacin_2,layer_Municipiosdemontaa_3,layer_SabanaNorte_4,layer_SabanaOccidente_5,layer_Provincias_Cundinamarca_7]);
